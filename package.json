{
  "name": "template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "zip-code": "npm run build && rm -f lambda.zip && cd dist && zip -qr ../lambda.zip index.js app/ && cd ..",
    "zip-nodemodules": "npm run pre:zip-nodemodules && zip -qr node_modules.zip nodejs && npm ci && rm -rf nodejs",
    "lint": "tslint --project tsconfig.json",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "quick-test": "mocha -r ts-node/register test/**/*.spec.ts",
    "pre:build": "npm cit && npm run lint",
    "build": "npm run pre:build && rm -rf dist && tsc",
    "start": "ts-node src/index.local.ts",
    "pre:zip-nodemodules": "npm prune --production && rm -f node_modules.zip && mkdir nodejs && cp -r node_modules nodejs",
    "clean": "rm -f lambda.zip node_modules.zip && rm -rf dist"
  },
  "keywords": [],
